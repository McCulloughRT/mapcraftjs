{
  "name": "planning-tools",
  "version": "1.0.0",
  "description": "Web components and modules for city planning apps (React + Leaflet)",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha './tests/*.test.js' --compilers js:babel-core/register",
    "lint": "eslint . --ext .js --ext .jsx",
    "prebuild": "rm -rf dist",
    "build": "babel --out-dir dist src",
    "watch": "watch 'npm run build' src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapcraftlabs/planning-tools.git"
  },
  "keywords": [
    "React",
    "Leaflet",
    "Maps",
    "Planning"
  ],
  "author": "Fletcher Foti (MapCraft)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/mapcraftlabs/planning-tools/issues"
  },
  "homepage": "https://github.com/mapcraftlabs/planning-tools#readme",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^2.4.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.2",
    "expect": "^1.19.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "watch": "^0.18.0"
  },
  "dependencies": {
    "d3": "^3.5.16",
    "underscore": "^1.8.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
